{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "installCommand": "npm install --force",
  "framework": "nextjs",
  "regions": ["iad1"],
  "cleanUrls": true,
  "github": {
    "silent": true
  },
  "rewrites": [
    { "source": "/storybook/(.*)", "destination": "/storybook-static/$1" }
  ],
  "outputFileTracing": true,
  "publicDirectory": "storybook-static"
} 